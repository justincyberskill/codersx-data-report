{
  "name": "lms-data-analysis",
  "version": "1.0.0",
  "repository": "git@github.codersx:justincorize/lms-data-analysis.git",
  "license": "MIT",
  "author": "Justin Jr <minh-thong.lu@cyberskill.tech>",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build-server",
    "build-server": "babel . -d build --ignore node_modules,build",
    "clean": "rm -rf build && mkdir build",
    "dev": "nodemon --inspect --exec babel-node app.js",
    "eslint-fix": "eslint --ignore-path .gitignore '**/*.{js,jsx}' --fix",
    "lint-fix": "yarn stylelint-fix & yarn eslint-fix & yarn prettier-fix",
    "prettier-fix": "prettier --ignore-path .gitignore '**/*.{js}' --write",
    "start": "node build/app.js"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "agenda": "^3.1.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "global": "^4.4.0",
    "googleapis": "^49.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.9.9"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4"
  },
  "engines": {
    "yarn": "1.x"
  }
}